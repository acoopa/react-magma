{
  "extends": "../../tsconfig",
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "rootDir": "./src",
    "removeComments": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "strictFunctionTypes": false,
    "strictNullChecks": false
  }
}